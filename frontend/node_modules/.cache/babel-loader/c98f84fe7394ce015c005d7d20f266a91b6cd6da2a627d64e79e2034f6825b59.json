{"ast":null,"code":"export default {\n  data() {\n    return {\n      startData: {\n        name: '',\n        quality: 'audio_only',\n        customPath: '',\n        blockAds: false\n      },\n      useDefaultFilename: true,\n      includeDate: false,\n      dummyProcesses: Array.from({\n        length: 10\n      }, (_, i) => ({\n        id: i + 1,\n        name: `Process #${i + 1}`,\n        data: `Data #${i + 1}`,\n        speed: `Speed #${i + 1}`\n      }))\n    };\n  },\n  methods: {\n    startStream() {\n      alert('Stream started!');\n    }\n  }\n};","map":{"version":3,"names":["data","startData","name","quality","customPath","blockAds","useDefaultFilename","includeDate","dummyProcesses","Array","from","length","_","i","id","speed","methods","startStream","alert"],"sources":["G:\\Informatik\\Projekte\\streamlink_web-ui\\frontend\\src\\components\\StreamlinkUi.vue"],"sourcesContent":["<template>\r\n    <div id=\"app\">\r\n      <div class=\"main-container\">\r\n        <!-- Main UI -->\r\n        <div class=\"streamlink-ui\">\r\n          <!-- Input Section -->\r\n          <div class=\"input-section\">\r\n            <div class=\"form-group\">\r\n              <label>Channel Name:</label>\r\n              <input v-model=\"startData.name\" placeholder=\"Enter Channel Name\" />\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label>Filename:</label>\r\n              <input type=\"checkbox\" v-model=\"useDefaultFilename\" />\r\n              Use default filename\r\n              <div v-if=\"!useDefaultFilename\" class=\"conditional-options\">\r\n                <input type=\"checkbox\" v-model=\"includeDate\" />\r\n                Include date in filename\r\n                <input v-model=\"startData.customPath\" placeholder=\"Enter custom filename or path\" />\r\n              </div>\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label>Quality:</label>\r\n              <select v-model=\"startData.quality\">\r\n                <option value=\"audio_only\">Audio Only</option>\r\n                <option value=\"best\">Best</option>\r\n                <option value=\"720p\">720p</option>\r\n                <option value=\"480p\">480p</option>\r\n                <option value=\"360p\">360p</option>\r\n                <option value=\"160p\">160p</option>\r\n              </select>\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <input type=\"checkbox\" v-model=\"startData.blockAds\" />\r\n              Block ads\r\n            </div>\r\n            <button class=\"start-button\" @click=\"startStream\">START</button>\r\n          </div>\r\n  \r\n          <!-- Running Processes -->\r\n          <div class=\"process-section\">\r\n            <h2>Services Running:</h2>\r\n            <div class=\"process-list\">\r\n              <div class=\"process-row\" v-for=\"process in dummyProcesses\" :key=\"process.id\">\r\n                <span>{{ process.name }}</span>\r\n                <span>{{ process.data }}</span>\r\n                <span>{{ process.speed }}</span>\r\n                <div class=\"action-buttons\">\r\n                  <button class=\"stop-button\">STOP</button>\r\n                  <button class=\"kill-button\">KILL</button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        startData: {\r\n          name: '',\r\n          quality: 'audio_only',\r\n          customPath: '',\r\n          blockAds: false,\r\n        },\r\n        useDefaultFilename: true,\r\n        includeDate: false,\r\n        dummyProcesses: Array.from({ length: 10 }, (_, i) => ({\r\n          id: i + 1,\r\n          name: `Process #${i + 1}`,\r\n          data: `Data #${i + 1}`,\r\n          speed: `Speed #${i + 1}`,\r\n        })),\r\n      };\r\n    },\r\n    methods: {\r\n      startStream() {\r\n        alert('Stream started!');\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Colors */\r\n  :root {\r\n    --twitch-purple: #9146ff;\r\n    --dark-grey: #2d2d2d;\r\n    --light-grey: #393939;\r\n    --hover-grey: #4d4d4d;\r\n    --background-gradient: linear-gradient(135deg, #9146ff, #2d2d2d);\r\n    --button-hover: #772ce8;\r\n    --stop-color: #ff6f61;\r\n    --kill-color: #ff3b3b;\r\n  }\r\n  \r\n  /* Full Page Background */\r\n  .main-container {\r\n    background: var(--background-gradient);\r\n    height: 100vh;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n  \r\n  /* Main UI */\r\n  .streamlink-ui {\r\n    background-color: var(--dark-grey);\r\n    padding: 20px;\r\n    border-radius: 20px;\r\n    color: white;\r\n    width: 80%;\r\n    max-width: 1200px;\r\n    box-shadow: 0px 10px 30px rgba(0, 0, 0, 0.4);\r\n    display: flex;\r\n    justify-content: space-between;\r\n  }\r\n  \r\n  /* Input Section */\r\n  .input-section {\r\n    flex: 1;\r\n    margin-right: 20px;\r\n  }\r\n  \r\n  .form-group {\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  input,\r\n  select {\r\n    width: 100%;\r\n    padding: 10px;\r\n    margin-top: 5px;\r\n    border-radius: 10px;\r\n    border: none;\r\n    background: var(--light-grey);\r\n    color: white;\r\n  }\r\n  \r\n  input:hover,\r\n  select:hover {\r\n    background: var(--hover-grey);\r\n  }\r\n  \r\n  .start-button {\r\n    background-color: var(--twitch-purple);\r\n    color: white;\r\n    border: none;\r\n    padding: 15px 20px;\r\n    border-radius: 10px;\r\n    font-size: 18px;\r\n    font-weight: bold;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n  }\r\n  \r\n  .start-button:hover {\r\n    background-color: var(--button-hover);\r\n  }\r\n  \r\n  /* Processes Section */\r\n  .process-section {\r\n    flex: 1;\r\n    background-color: var(--light-grey);\r\n    padding: 20px;\r\n    border-radius: 10px;\r\n  }\r\n  \r\n  .process-list {\r\n    margin-top: 10px;\r\n  }\r\n  \r\n  .process-row {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: 10px 0;\r\n    border-bottom: 1px solid var(--dark-grey);\r\n  }\r\n  \r\n  .action-buttons {\r\n    display: flex;\r\n    gap: 10px;\r\n  }\r\n  \r\n  .stop-button,\r\n  .kill-button {\r\n    padding: 10px 15px;\r\n    border: none;\r\n    border-radius: 10px;\r\n    color: white;\r\n    font-weight: bold;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .stop-button {\r\n    background-color: var(--stop-color);\r\n  }\r\n  \r\n  .stop-button:hover {\r\n    background-color: #ff4a3b;\r\n  }\r\n  \r\n  .kill-button {\r\n    background-color: var(--kill-color);\r\n  }\r\n  \r\n  .kill-button:hover {\r\n    background-color: #ff1a1a;\r\n  }\r\n  </style>\r\n  "],"mappings":"AA4DE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;QACTC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,YAAY;QACrBC,UAAU,EAAE,EAAE;QACdC,QAAQ,EAAE;MACZ,CAAC;MACDC,kBAAkB,EAAE,IAAI;MACxBC,WAAW,EAAE,KAAK;MAClBC,cAAc,EAAEC,KAAK,CAACC,IAAI,CAAC;QAAEC,MAAM,EAAE;MAAG,CAAC,EAAE,CAACC,CAAC,EAAEC,CAAC,MAAM;QACpDC,EAAE,EAAED,CAAA,GAAI,CAAC;QACTX,IAAI,EAAE,YAAYW,CAAA,GAAI,CAAC,EAAE;QACzBb,IAAI,EAAE,SAASa,CAAA,GAAI,CAAC,EAAE;QACtBE,KAAK,EAAE,UAAUF,CAAA,GAAI,CAAC;MACxB,CAAC,CAAC;IACJ,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZC,KAAK,CAAC,iBAAiB,CAAC;IAC1B;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}